<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>PdWebApp</display-name>

	<servlet>
		<display-name>form1Servlet</display-name>
		<servlet-name>form1Servlet</servlet-name>
		<servlet-class>pw.inz.pd.web.servlets.Form1Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>form1Servlet</servlet-name>
		<url-pattern>/form1Servlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>form2Servlet</display-name>
		<servlet-name>form2Servlet</servlet-name>
		<servlet-class>pw.inz.pd.web.servlets.Form2Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>form2Servlet</servlet-name>
		<url-pattern>/form2Servlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>